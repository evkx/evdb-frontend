{
  "name": "react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "vite & wait-on http://localhost:5173 & cypress open",
    "coverage": "cypress run --component",
    "coverage:e2e": "vite & wait-on http://localhost:5173 & cypress run",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@altinn/altinn-design-system": "^0.29.1",
    "@digdir/design-system-react": "^0.33.0",
    "@digdir/design-system-tokens": "0.8.1",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.5.1",
    "classnames": "^2.3.2",
    "cypress": "^13.3.0",
    "cypress-real-events": "^1.10.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "i18next": "^23.5.1",
    "lint-staged": "^14.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.16.0",
    "redux-logger": "^3.0.6",
    "vite-plugin-svgr": "^4.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@types/redux-logger": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "6.0.2",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "typescript": "5.2.2",
    "vite": "^4.4.11"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs}": "yarn lint --cache --fix",
    "*.{css,md,mdx,json}": "yarn prettier --write"
  },
  "packageManager": "yarn@3.3.1"
}
