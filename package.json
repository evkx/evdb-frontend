{
  "name": "react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "vite & wait-on http://localhost:5173 & cypress open",
    "coverage": "cypress run --component",
    "coverage:e2e": "vite & wait-on http://localhost:5173 & cypress run",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@altinn/altinn-design-system": "^0.30.0",
    "@digdir/designsystemet-css": "0.10.0",
    "@digdir/designsystemet-react": "0.63.1",
    "@digdir/designsystemet-theme": "^0.15.3",
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "i18next": "^24.2.0",
    "lint-staged": "^15.2.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.2.0",
    "react-query": "^3.39.3",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.1",
    "redux-logger": "^3.0.6",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-cypress": "^4.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^7.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.9.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript": "5.4.5",
    "vite": "^5.2.11"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs}": "yarn lint --cache --fix",
    "*.{css,md,mdx,json}": "yarn prettier --write"
  },
  "packageManager": "yarn@4.5.3"
}
